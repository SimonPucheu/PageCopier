{
    "name": "Page Copier",
    "action": {},
    "manifest_version": 3,
    "version": "1.0.0",
    "description": "Copy the current page on a server to have access to the page as a permanant file",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "scripts/background.js"
    },
 "icons": {
    "16": "icons/16.16.png",
    "32": "icons/32.32.png",
    "48": "icons/48.48.png",
    "128": "icons/128.128.png"
  }
}